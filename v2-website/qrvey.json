{
  "index_name": "staging_qrvey",
  "start_urls": ["http://partners-staging.qrvey.com/"],
  "stop_urls": [],
  "sitemap_urls": ["http://partners-staging.qrvey.com/sitemap.xml"],
  "selectors": {
    "lvl0": {
      "selector": "//*[contains(@class,'navGroups')]//*[contains(@class,'navListItemActive')]/preceding::h3[1]",
      "type": "xpath",
      "global": true,
      "default_value": "Blog"
    },
    "lvl1": ".post h1",
    "lvl2": ".post h2",
    "lvl3": ".post h3",
    "lvl4": ".post h4",
    "text": ".post article p, .post article li"
  },
  "selectors_exclude": [".hash-link"],
  "custom_settings": {
    "attributesForFaceting": ["language", "version", "tags", "type"],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  }
}
